{
  "name": "@rxts/vue",
  "version": "0.0.0",
  "description": "Make Vue greater with RxTS.",
  "repository": "git@github.com:rx-ts/vue.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "run-s build:r build:ts build:docs",
    "build:docs": "vuepress build",
    "build:r": "r -e named -p",
    "build:ts": "tsc -b",
    "clean": "rimraf dist packages/*/{lib,*.tsbuildinfo}",
    "dev": "vuepress dev",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext js,md,mdx,ts,tsx,vue -f friendly",
    "lint:style": "stylelint --cache '.vuepress/**/*.{scss,vue}'",
    "lint:tsc": "tsc",
    "postinstall": "yarn-deduplicate || exit 0",
    "start": "serve dist",
    "test": "cross-env NODE_ENV=development jest",
    "type-coverage": "type-coverage --cache --detail --ignore-catch --ignore-files '**/*.d.ts' --update"
  },
  "devDependencies": {
    "@1stg/app-config": "^0.5.5",
    "@1stg/lib-config": "^0.5.5",
    "@d-ts/vue": "^0.4.3",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.7",
    "@types/qrcode": "^1.3.5",
    "@types/webpack": "^4.41.25",
    "@vue/test-utils": "^1.1.1",
    "lerna": "^3.22.1",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "qrcode": "^1.4.4",
    "qrious": "^4.0.2",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "serve": "^11.3.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "type-coverage": "^2.14.2",
    "typescript": "^4.0.5",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.7.1",
    "vuepress-plugin-typescript": "^0.3.1",
    "vuepress-types": "^0.9.4",
    "yarn-deduplicate": "^3.1.0"
  },
  "resolutions": {
    "@babel/core": "^7.12.3",
    "imagemin-gifsicle": "^6.0.1",
    "prettier": "^2.1.2",
    "webpack-chain": "^6.5.1"
  },
  "babel": {
    "presets": [
      [
        "@1stg",
        {
          "typescript": true,
          "vue": true
        }
      ]
    ]
  },
  "jest": {
    "collectCoverage": true,
    "preset": "ts-jest"
  },
  "typeCoverage": {
    "atLeast": 99.21
  }
}
